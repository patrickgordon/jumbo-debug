{
	"manifest_version": 2,
	"name": "Jumbo Debug",
	"version": "0.1",
	"description": "A debug tool for sites Powered by Jumbo.",
	"author": "Patrick Gordon",
	"content_scripts": [
		{
			"matches": [
				"http://*.dev.lan/*",
				"https://*.dev.lan/*",
				"https://*.jumdev.com/*",
				"https://ozlotteries.com/*",
				"https://materlotteries.com.au/*"
			],
			"js": [
				"jquery-3.4.1.min.js",
				"content.js"
			]
		}
	],
	"icons": {
		"16": "debug.png",
		"48": "debug.png",
		"32": "debug.png",
		"128": "debug.png"
	  },
	"permissions": [
		"webNavigation"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "logo.png"
	}
}