{
	"manifest_version": 2,
	"name": "Jumbo Debug",
	"version": "0.3",
	"description": "A debug tool for sites Powered by Jumbo.",
	"author": "Patrick Gordon",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"jquery-3.4.1.min.js",
				"content.js"
			]
		}
	],
	"icons": {
		"16": "debug.png",
		"48": "debug.png",
		"32": "debug.png",
		"128": "debug.png"
	},
	"permissions": [],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "logo.png"
	}
}